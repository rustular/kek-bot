datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
      shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
    provider = "cargo prisma"
    output   = "../src/prisma.rs"
}

model KekUsage {
    id     DateTime @id @default(now())
    kek String
    user String
}
